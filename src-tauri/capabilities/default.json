{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:allow-read-text-file",
      "allow": [
        { "path": "$HOME/.steam/steam/steamapps/compatdata/1869590/pfx/drive_c/users/steamuser/AppData/Local/OmegaStrikers/identity.json" },
        { "path": "$HOME/Library/Application Support/CrossOver/Bottles/Crab Champions/drive_c/users/crossover/AppData/Local/OmegaStrikers/identity.json" },
        { "path": "$APPLOCALDATA/OmegaStrikers/identity.json" }
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [
        { "path": "$HOME/.steam/steam/steamapps/compatdata/1869590/pfx/drive_c/users/steamuser/AppData/Local/OmegaStrikers/identity.json" },
        { "path": "$HOME/Library/Application Support/CrossOver/Bottles/Crab Champions/drive_c/users/crossover/AppData/Local/OmegaStrikers/identity.json" },
        { "path": "$APPLOCALDATA/OmegaStrikers/identity.json" }
      ]
    },
    "os:default",
    "process:default"
  ]
}